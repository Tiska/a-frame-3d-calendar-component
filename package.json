{
  "name": "aframe-3d-calendar-component",
  "version": "1.0.2",
  "description": "A A-Frame 3D Calendar component for A-Frame.",
  "main": "index.js",
  "unpkg": "dist/aframe-3d-calendar-component.min.js",
  "scripts": {
    "dev": "budo index.js:dist/aframe-3d-calendar-component.min.js --port 7000 --live --open",
    "dist": "NODE_ENV=production & webpack -p --config webpack.config.js",
    "lint": "semistandard -v | snazzy",
    "prepublish": "npm run dist",
    "ghpages": "ghpages",
    "start": "npm run dev",
    "test": "karma start ./tests/karma.conf.js",
    "test:firefox": "karma start ./tests/karma.conf.js --browsers Firefox",
    "test:chrome": "karma start ./tests/karma.conf.js --browsers Chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tiska/aframe-3d-calendar-component.git"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "aframe-vr",
    "vr",
    "mozvr",
    "webvr",
    "a-frame-3D-calendar"
  ],
  "author": "abody",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tiska/aframe-3d-calendar-component/issues"
  },
  "homepage": "https://github.com/Tiska/aframe-3d-calendar-component#readme",
  "devDependencies": {
    "aframe": "*",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^13.0.0",
    "budo": "^8.2.2",
    "chai": "^3.4.1",
    "chai-shallow-deep-equal": "^1.3.0",
    "ghpages": "^0.0.8",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-chai-shallow-deep-equal": "0.0.4",
    "karma-chrome-launcher": "2.0.0",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-sinon-chai": "^1.1.0",
    "mocha": "^2.3.4",
    "randomcolor": "^0.4.4",
    "semistandard": "^8.0.0",
    "shelljs": "^0.7.0",
    "shx": "^0.1.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "snazzy": "^4.0.0",
    "uglifyjs-webpack-plugin": "^1.1.0",
    "webpack": "^3.8.1"
  },
  "semistandard": {
    "globals": [
      "AFRAME",
      "THREE"
    ],
    "ignore": [
      "examples/build.js",
      "dist/**"
    ]
  },
  "dependencies": {
    "moment": "^2.19.2"
  }
}
